    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./compiled",
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es2015"
  },
  "exclude": [
    "node_modules"
  ]
}
